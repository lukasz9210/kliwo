{
  "name": "public",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "fast-sort": "^2.1.1",
    "jquery": "^3.4.1",
    "npm-watch": "^0.6.0",
    "rc-slider": "^8.7.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "watch": "npm-watch",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "npm run build-rename-runtime-js && npm run build-rename-main-js && npm run build-rename-2-js",
    "build-rename-runtime-js": "renamer --find '/runtime-main\\.[^\\.]+\\.js/i' --replace 'runtime-main.js' build/static/js/*.js",
    "build-rename-main-js": "renamer --find '/main\\.[^\\.]+\\.chunk.js/i' --replace 'main.js' build/static/js/*.js",
    "build-rename-2-js": "renamer --find '/2\\.[^\\.]+\\.chunk.js/i' --replace '2.js' build/static/js/*.js",
    "build-rename-css": "renamer --find '/main\\.[^\\.]+\\.chunk.css/i' --replace 'main.css' build/static/css/*.css"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "renamer": "^1.1.4"
  }
}
